class Solution {
    public void rotate(int[][] matrix) {
        int n = matrix.length;
        int m = matrix[0].length;
        int newm[][] = new int[n][m];
        for(int i=0; i<n ;i++){
            for(int j=0; j<m ;j++){
                newm[j][n-i-1] = matrix[i][j];
            }
        }
        for(int i=0; i<n ;i++){
            for(int j=0; j<m ;j++){
                matrix[i][j] = newm[i][j];
            }
        }
    }
}

//Brute Force without in-place
