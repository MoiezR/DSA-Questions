//Brute Force

public class Solution {
    // DO NOT MODIFY THE ARGUMENTS WITH "final" PREFIX. IT IS READ ONLY
    public int[] repeatedNumber(final int[] A) {
        int ans[] = new int[2];
        int n = A.length;
        int hash[] = new int[n+1];
        for(int i = 0; i<n ;i++){
            hash[A[i]]++;
        }
        for(int i=1; i<=n; i++){
            int count = hash[i];
            if(count==2) ans[0] = i;
            if(count==0) ans[1] = i;
        }
        return ans;
    }
}

