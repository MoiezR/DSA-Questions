class Solution {
    public int maxArea(int[] height) {
        int n = height.length;
        int maxArea = 0;
        for(int i=0; i<n-1 ; i++){
            for(int j=i+1; j<n; j++){
                int length = j-i;
                int breadth = Math.min(height[i], height[j]);
                int area = length*breadth;
                maxArea = Math.max(area, maxArea);
            }
        }
        return maxArea;
    }
}

//Brute Force
