//Brute Force Solution

class Solution {
    public int[] productExceptSelf(int[] nums) {
        int n = nums.length;
        int answer[] = new int[n];
        for(int i = 0; i<n ; i++){
            int prod = 1;
            for(int j = 0; j<n ; j++){
                if(i==j) continue;
                prod *= nums[j];
            }
            answer[i]=prod;
        }
        return answer;
    }
}
