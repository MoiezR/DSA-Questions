/**
 * Definition for singly-linked list.
 * class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    public boolean hasCycle(ListNode head) {
        ListNode curr = head;
        HashSet<Integer> hs=new HashSet<>(); 
        while(curr!=null){
            if(hs.contains(curr.val)){
                return true;
            }
            hs.add(curr.val);
            curr=curr.next;
        }
        return false;
    }
}
